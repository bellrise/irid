// Print hex value to console.
func putx(int val);

// Select a device for the I/O library.
func iosel(int dev);

// Halt the system
func halt();

func _entry() [naked, local] {
    // Setup stack before calling main
    __asm("mov sp, 0x8000");
    __asm("mov bp, 0x8000");

    iosel(0x1000);
    main();

    // Do nothing.
    halt();
}

func main() -> int {
    let int a;

    a = 0x1234;

    putx(a);
}

func other() {
    let int b;
}
